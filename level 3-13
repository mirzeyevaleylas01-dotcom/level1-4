#include <iostream>
using namespace std;

int main() {
    int n, original, rev = 0;
    cout << "Ededi daxil edin: ";
    cin >> n;
    original = n;
    while(n != 0) {
        rev = rev * 10 + n % 10;
        n /= 10;
    }
    if(rev == original)
        cout << "Palindrome ededdir";
    else
        cout << "Palindrome deyil";
    return 0;
}
