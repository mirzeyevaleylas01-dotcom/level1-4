#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Nece eded daxil edeceksiniz? ";
    cin >> n;
    int x, tekSum = 0, cutSum = 0;
    for (int i = 0; i < n; i++) {
        cin >> x;
        if (i % 2 == 0) cutSum += x;
        else tekSum += x;
    }
    cout << "Cut indeksli cem: " << cutSum << "\nTek indeksli cem: " << tekSum;
    return 0;
}
