#include <iostream>
using namespace std;

int main() {
    int x;
    cout << "Bir eded daxil edin: ";
    cin >> x;

    if (x > 0)
        cout << "Eded musbetdir." << endl;
    else if (x < 0)
        cout << "Eded menfidir." << endl;
    else
        cout << "Eded sifirdir." << endl;

    return 0;
}
