#include <iostream>
using namespace std;

int main() {
    double C, F;
    cout << "Selsi temperaturunu daxil edin: ";
    cin >> C;
    F = (C * 9 / 5) + 32;
    cout << "Fahrenheit: " << F << endl;

    cout << "Fahrenheit temperaturunu daxil edin: ";
    cin >> F;
    C = (F - 32) * 5 / 9;
    cout << "Selsi: " << C << endl;
    return 0;
}
